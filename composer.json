{
    "name": "petrepatrasc/starcraft-connection-layer",
    "type": "symfony-bundle",
    "description": "A PHP 5.3+ cURL wrapper that allows easy access to popular Starcraft 2 service APIs.",
    "keywords": ["PHP", "Starcraft 2", "API", "Symfony"],
    "homepage": "http://petrepatrasc.github.io/starcraft-connection-layer",
    "license": "MIT",
    "authors": [
        {
            "name": "Petre Pătrașc",
            "email": "petre@dreamlabs.ro",
            "homepage": "http://petrepatrasc.github.io"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "symfony/framework-bundle": ">=2.1"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "petrepatrasc\\StarcraftConnectionLayerBundle": ""
        }
    },
    "target-dir": "petrepatrasc/StarcraftConnectionLayerBundle"
}
